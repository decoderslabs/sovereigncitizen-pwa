{
  "version": 2,
  "name": "sovereigncitizen",
  "rewrites": [
    { "source": "/", "destination": "/jobko/index.html" },
    { "source": "/:path*", "destination": "/jobko/:path*" }
  ],
  "headers": [
    {
      "source": "/jobko/_service-worker.js",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" },
        { "key": "Service-Worker-Allowed", "value": "/jobko/" }
      ]
    },
    {
      "source": "/jobko/_manifest.json",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ]
}


